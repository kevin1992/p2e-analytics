{
  "root": true,
  "extends": [
    "eslint:recommended",
    "next/core-web-vitals"
  ],
  "parser": "@typescript-eslint/parser",
  "rules": {
    "consistent-return": 2,
    "indent": [
      1,
      2
    ],
    "no-else-return": 1,
    "semi": [
      1,
      "always"
    ],
    "space-unary-ops": 2
  }
}
